<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.iesvirgendelcarmen.dam.ejercicio_01a.Ejercicio_01A"
    android:orientation="vertical"
    android:gravity="center"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QUINTA PANTALLA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/boton01EB2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PASAR A SEGUNDA PANTALLA"
        android:onClick="boton1e"
        />

    <Button
        android:id="@+id/boton01ED4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PASAR A CUARTA PANTALLA"
        android:onClick="boton2e"
        />

</LinearLayout>
