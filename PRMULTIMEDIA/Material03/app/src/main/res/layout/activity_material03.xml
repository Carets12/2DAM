<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#c2d1fc"
    tools:context="es.vcarmen.material03.Material03">

    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab1"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="180dp"
        android:src="@drawable/emoticon"
        android:layout_marginRight="16dp"
        android:visibility="invisible"
        app:backgroundTint="#1100ffe1"
        />

    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab2"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="100dp"
        android:layout_marginRight="16dp"
        android:src="@drawable/emoticon"
        android:visibility="invisible"
        app:backgroundTint="#1103ff43"
        />

    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab3"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="16dp"
        android:src="@drawable/emoticon"
        android:layout_marginRight="16dp"
        app:fabSize="normal"
        app:backgroundTint="#11ff0008"

        />

    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab21"
        android:layout_gravity="left|end"
        android:layout_marginBottom="100dp"
        android:layout_marginRight="16dp"
        android:src="@drawable/emoticon"
        android:visibility="visible"
        app:backgroundTint="#1103ff43"
        />



    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab22"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="100dp"
        android:layout_marginRight="16dp"
        android:src="@drawable/emoticon"
        android:visibility="invisible"
        app:backgroundTint="#1103ff43"
        />



    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab23"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="100dp"
        android:layout_marginRight="16dp"
        android:src="@drawable/emoticon"
        android:visibility="invisible"
        app:backgroundTint="#1103ff43"
        />



    <android.support.design.widget.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fab24"
        android:layout_gravity="bottom|end"
        android:layout_marginBottom="100dp"
        android:layout_marginRight="16dp"
        android:src="@drawable/emoticon"
        android:visibility="invisible"
        app:backgroundTint="#1103ff43"
        />





</FrameLayout>
